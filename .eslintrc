{
	"extends": ["oclif", "oclif-typescript", "eslint:recommended", "prettier"],
	"rules": {
		"no-unused-vars": "off",
		"radix": ["error", "as-needed"],
		"@typescript-eslint/explicit-module-boundary-types": "off",
		"@typescript-eslint/no-unused-vars": [
			"error", // or "error"
			{
				"argsIgnorePattern": "^_",
				"varsIgnorePattern": "^_",
				"caughtErrorsIgnorePattern": "^_"
			}
		]
	}
}
